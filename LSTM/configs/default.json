{
  "name": "default_experiment",
  "dataset": {
    "n_tickers": 20,
    "start_date": "2013-01-01",
    "end_date": "2022-12-31"
  },
  "loaders": {
    "window_size": 10,
    "batch_size": 4,
    "shuffle": true,
    "n_workers": 2,
    "val_ratio": 0.15,
    "test_ratio": 0.15
  },
  "experiment": {
    "n_epochs": 40,
    "learning_rate": 5e-4,
    "patience": 2
  },
  "model": {
    "window_size": 10,
    "lstm_hidden_size": 20,
    "n_lstm_layers": 2,
    "dense_hidden_sizes": 10,
    "n_dense_layers": 1,
    "dropout": 0.0,
    "dropout_lstm": false
  }
}